                                        <assignee username="-1">Unassigned</assignee>
                                    <reporter username="mteodoro">Mark Teodoro</reporter>
                        <labels>
                            <label>patch</label>
                    </labels>
                <created>Fri, 20 Sep 2013 17:57:12 +0000</created>
                <updated>Thu, 23 Jan 2014 21:58:56 +0000</updated>
