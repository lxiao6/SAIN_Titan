     &amp;lt;osgi.import&amp;gt;&lt;br/&gt;
       !org.apache.avro*,&lt;br/&gt;
+      !com.google.common*,&lt;br/&gt;
       com.thoughtworks.paranamer,&lt;br/&gt;
       org.codehaus.jackson*,&lt;br/&gt;
       org.xerial.snappy;resolution:=optional,&lt;/p&gt;




